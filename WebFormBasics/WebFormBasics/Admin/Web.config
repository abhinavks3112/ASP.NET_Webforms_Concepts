<?xml version="1.0"?>
<configuration>
  <system.web>
	  <authorization>
		  <allow roles="Administrators"/>
		  <deny users="*"/>
	  </authorization>
	  <!-- We can impersonate other user who is not of above role also-->
	  <identity impersonate="true" userName="AnyOtherUserThanThisUser" password="password"/>
  </system.web>
</configuration>
